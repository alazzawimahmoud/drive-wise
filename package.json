{
  "name": "drive-wise",
  "version": "1.0.0",
  "type": "module",
  "description": "Belgian driving license theory questions API",
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "build": "(npm install --no-save @rollup/rollup-linux-x64-gnu 2>/dev/null || true) && vite build && tsc -p tsconfig.node.json",
    "start": "npm run db:init && NODE_ENV=production node dist/server/index.js",
    "preview": "NODE_ENV=production node dist/server/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:init": "tsx scripts/db-init.ts",
    "db:studio": "drizzle-kit studio",
    "data:cleanup": "tsx scripts/cleanup.ts",
    "data:rephrase": "tsx scripts/rephrase.ts",
    "data:validate": "tsx scripts/validate.ts",
    "data:seed": "tsx scripts/seed.ts",
    "data:pipeline": "npm run data:cleanup && npm run data:rephrase && npm run data:validate && npm run data:seed"
  },
  "keywords": [
    "driving",
    "license",
    "theory",
    "belgium",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@tanstack/react-query": "^5.90.16",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@xstate/react": "^6.0.0",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.26",
    "he": "^1.2.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "p-limit": "^6.2.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.19.4",
    "vite": "^7.3.0",
    "vite-express": "^0.19.0",
    "xstate": "^5.25.0",
    "zod": "^3.25.34"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/he": "^1.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.30",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "typescript": "^5.8.3"
  }
}
